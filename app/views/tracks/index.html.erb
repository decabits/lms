<h1>Tracks</h1>

<table>
  <% @tracks.each do |track| %>
    <tr>
      <td><%= link_to track.name, track_path(track) %></td>
      <td><%= link_to 'Edit', edit_track_path(track) %></td>
      <td><%= link_to 'Delete', edit_track_path(track), method: :delete, data: {confirm: "Are you sure?"} %></td>
    </tr>
  <% end %>
</table>
